<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.1.1//EN" "http://www.opensymphony.com/xwork/xwork-1.1.1.dtd">
<xwork>
	<package name="badevalua" extends="webwork-default">
		<action name="badList" class="badEvaluationAction" method="badList">
			<result name="list">/evaluation/badEvaluationList.jsp</result>
		</action>
		<action name="detailBad" class="badEvaluationAction" method="detailBad">
			<result name="detail">/evaluation/detailBadEvalua.jsp</result>
		</action>
		<action name="getImg" class="badEvaluationAction" method="getImg"/>
		<!-- 差评信息查询 -->
		<action name="badCount" class="badEvaluationAction" method="badCount">
			<result name="count">/count/getBadNumberInfo_Count.jsp</result>
		</action>
		<action name="badCountDetail" class="badEvaluationAction" method="badCountDetail">
			<result name="detail">/count/getBadNumberInfo_Detail.jsp</result>
		</action>
		<action name="updateBadEvaluationSeason" class="badEvaluationAction" method="updateBadEvaluationSeason">
			<result name="update">/message.jsp</result>
		</action>
	</package>
</xwork>