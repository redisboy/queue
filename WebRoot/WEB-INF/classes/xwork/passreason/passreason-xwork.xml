<!DOCTYPE xwork PUBLIC "-//OpenSymphony Group//XWork 1.0//EN"
"http://www.opensymphony.com/xwork/xwork-1.0.dtd">
<!-- xwork需要分模块配置action -->
<xwork>
	<package name="passreason" extends="default-webwork">
		
		<action  name="selectPassReason"  class="passReasonAction"  method="selectPassReason">
			<result name="success">/passreason/passReasonList.jsp</result>
		</action>
		<action  name="addPassReason"  class="passReasonAction"  method="addPassReason">
			<result name="success">/passreason/addPassReason.jsp</result>
		</action>
		<action  name="savePassReason"  class="passReasonAction"  method="savePassReason">
			<result name="success">/message.jsp</result>
		</action>
		<action  name="deletePassReason"  class="passReasonAction"  method="deletePassReason">
			<result name="success">/message.jsp</result>
		</action>
		<action  name="toUpdatePassReason"  class="passReasonAction"  method="toUpdatePassReason">
			<result name="success">/passreason/updatePassReason.jsp</result>
		</action>
		<action  name="updatePassReasonById"  class="passReasonAction"  method="updatePassReasonById">
			<result name="success">/message.jsp</result>
		</action>
		
	</package>
</xwork>